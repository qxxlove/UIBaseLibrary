<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bool.jetpackmvvm">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".room.RoomActivity"></activity>
        <activity android:name=".pageing.PageingActivity" />
        <activity android:name=".workmanager.WorkManagerActivity" />
        <activity android:name=".lifecycle.LifecycleActivity" />
        <activity android:name=".navigation.TestActivity" />
        <activity android:name=".navigation.NavHostActivity" />
        <activity android:name=".viewmodel.ui.LeftAndRightActivity" />
        <activity android:name=".viewmodel.ui.TimerActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".lifecycle.MyLifeService" />
    </application>

</manifest>